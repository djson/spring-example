<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sample.demo.repository.SampleRepo">

    <select id="getUser" parameterType="HashMap" resultType="SampleUser">
        SELECT
            USER_ID
            ,USER_NAME
            ,USER_AGE
            ,USER_JOIN_DATE
        FROM
            SAMPLE_USER
    </select>
    
</mapper>